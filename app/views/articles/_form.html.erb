<%= form_with model: article do |f| %>
  <div class="mb-3">
    <%= f.label :title, class: "form-label" %>
    <%= f.text_field :title , class: "form-control"%>
  </div>
  <div class="mb-3">
    <%= f.label :content, class: "form-label"%>
    <%= f.text_field :content, class: "form-control ace_text-input" %>
  </div>
  <div >
    <%= f.submit %>
  </div>

<% end %>
